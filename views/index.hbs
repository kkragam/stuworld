 
<div class="container-fluid">
  
 <div ><h2 class="text-center">{{data.title}}</h2></div> 
  
{{#if data.carousel}}
<div id="myCarousel" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators"> 
       {{#each data.carousel}}
             {{#if @first}}
                    <li data-target="#myCarousel" data-slide-to="{{@key}}" class="active"></li>
              {{else}}
                    <li data-target="#myCarousel" data-slide-to="{{@key}}"></li>
              {{/if}}           
       {{/each}} 
  </ol> 
  <!-- Wrapper for slides -->
  <div class="carousel-inner" role="listbox"> 
    {{#each data.carousel}}
     {{#if @first}}
      <div class="item active">
      <img src="{{img}}" alt="{{alt}}" height="{{height}}" width="{{width}}" class="img-responsive img-rounded">
    <!--  <div class="{{img}} sprite" height="{{height}}" width="{{width}}"></div>-->
      <!--<div class="container">-->
            <div class="carousel-caption">
              <h2>{{title}}</h2>
              <p>{{desc}}</p>
                <p><a class="btn btn-primary" href="{{buttonurl}}" role="button">{{buttonvalue}}</a></p>
            </div>
          <!--</div>-->
    </div>
    {{else}}
    <div class="item">
      <img src="{{img}}" alt="{{alt}}" height="{{height}}" width="{{width}}" class="img-responsive img-rounded">
      <!--<div class="{{img}} sprite" height="{{height}}" width="{{width}}"></div>-->
    <!--  <div class="container">-->
            <div class="carousel-caption">
              <h2>{{title}}</h2>
              <p>{{desc}}</p>
               <p><a class="btn btn-primary" href="{{buttonurl}}" role="button">{{buttonvalue}}</a></p>
            </div>
          <!--</div>-->
    </div> 
    {{/if}}
    {{/each}} 
  </div> 
  <div>
  </div> 
  <!-- Left and right controls -->
  <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
{{/if}}
 
</div>